#include<iostream>
using namespace std;
int main(){
    int n;
    cout<<"Enter a number : ";
    cin>>n;
    for (int i = 0; i <= n; i++)
    {
        int next = 1;
        for (int j = 0; j <= i; j++)
        {
            cout<<next<<' ';
            next = (i-j)*next/(j+1);
        }
        cout<<endl;
        
    }
    
    return 0;
}
